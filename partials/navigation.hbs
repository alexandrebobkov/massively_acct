<nav id="nav">
    <ul class="links">
        {{#foreach navigation}}
        <li class="nav-{{slug}} {{#if current}}active{{/if}}"><a href="{{url absolute="true"}}">{{label}}</a></li>
        {{/foreach}}
    </ul>
    <ul class="icons">
        {{#if @custom.url_LinkedIn}}
        <li><a href="{{@custom.url_LinkedIn}}" target="_blank" class="icon fa-linkedin" title="LinkedIn"><span class="label">LinkedIn</span></a></li>
        {{/if}}
        {{#if @custom.url_facebook}}
        <li><a href="{{@custom.url_facebook}}" target="_blank" class="icon fa-facebook" title="Facebook"><span class="label">Facebook</span></a></li>
        {{/if}}
        {{#if @site.twitter}}
        <li><a href="{{twitter_url @site.twitter}}" target="_blank" class="icon fa-twitter" title="Twitter"><span class="label">Twitter</span></a></li>
        {{/if}}

    </ul>
</nav>

{{!-- @custom.website_name --}}
